{
    "name": "jjuanrivvera99/canvas-lms-kit",
    "description": "A PHP library for the Canvas LMS API",
    "keywords": [
        "canvas",
        "canvas-lms",
        "canvas-lms-api",
        "canvas-lms-php",
        "canvas-lms-php-api",
        "canvas-lms-php-library",
        "canvas-lms-php-sdk",
        "canvas-lms-php-wrapper",
        "canvas-lms-sdk",
        "canvas-lms-wrapper",
        "canvas-php",
        "canvas-php-api",
        "canvas-php-library",
        "canvas-php-sdk",
        "canvas-php-wrapper",
        "canvas-sdk",
        "canvas-wrapper",
        "lms",
        "lms-api",
        "lms-php",
        "lms-php-api",
        "lms-php-library",
        "lms-php-sdk",
        "lms-php-wrapper",
        "lms-sdk",
        "lms-wrapper",
        "php",
        "php-api",
        "php-library",
        "php-sdk",
        "php-wrapper",
        "sdk",
        "wrapper"
    ],
    "version": "0.0.1",
    "type": "library",
    "scripts": {
        "check": [
            "@cs-fix",
            "@cs",
            "@phpstan",
            "@test"
        ],
        "test": "phpunit tests --testdox",
        "cs": "phpcs --standard=PSR12 src",
        "cs-fix": "phpcbf --standard=PSR12 src",
        "phpstan": "phpstan"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
		"sort-packages": true,
        "platform": {
            "php": "8.1"
        }
	},
    "license": "MIT",
    "authors": [
        {
            "name": "jjuanrivvera99",
            "email": "jjuanrivvera@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "psr/log": "^3.0",
        "ext-json": "*",
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "CanvasLMS\\": "src/"
        },
        "files": [
            "src/Utilities/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
